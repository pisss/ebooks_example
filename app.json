{
  "name": "BooDooBooks",
  "description": "Turn-key _ebooks deployment using twitter_ebooks",
  "repository": "https://github.com/BooDoo/ebooks_example/tree/persist-ckoudinary",
  "keywords": [
    "twitter",
    "bot",
    "ebooks"
  ],
  "addons": [
    "papertrail:choklad",
    "cloudinary:starter"
  ],
  "env": {
    "LANG": "en_US.UTF-8",
    "BOT_NAME": "",
    "OWNER_ACCOUNT": {
      "required": false,
      "value": ""
    },
    "SOURCE_USERNAME": {
      "required": false,
      "value": ""
    },
    "CONSUMER_KEY": "",
    "CONSUMER_SECRET": "",
    "ACCESS_TOKEN": "",
    "ACCESS_TOKEN_SECRET": "",
    "INITIAL_CORPUS_FILE": {
      "required": false,
      "value": "tweets.csv"
    },
    "CLOUDINARY_URL": {
      "required": false,
      "value": ""
    },
    "TWEET_INTERVAL": "2h",
    "TWEET_CHANCE": "0.8",
    "BLACKLIST": {
      "required": false,
      "value": "tinysubversions, dril"
    },
    "BANNED_TERMS": {
      "required": false,
      "value": "voldemort, evgeny morozov, heroku"
    },
    "SPECIAL_TERMS": {
      "required": false,
      "value": ""
    },
    "DEFAULT_DELAY": "5..60",
    "DM_DELAY": {
      "required": false,
      "value": "0"
    },
    "MENTION_DELAY": {
      "required": false,
      "value": "5..60"
    },
    "TIMELINE_DELAY": {
      "required": false,
      "value": "10..600"
    },
    "TIMEOUT_SLEEP": "5",
    "MAX_ERROR_RETRIES": "10",
    "UPDATE_FOLLOWS_INTERVAL": "90m",
    "UPDATE_ARCHIVE_INTERVAL": "24h"
  }
}
